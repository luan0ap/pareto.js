{
  "name": "paretojs",
  "version": "1.0.0",
  "description": "An extremely small, intuitive and fast functional utility library for JavaScript",
  "scripts": {
    "test": "jest --watch",
    "dev": "webpack -w --mode development --module-bind js=babel-loader --output-path='./dist'",
    "build": "webpack --mode production --module-bind js=babel-loader"
  },
  "keywords": [
    "util",
    "utility",
    "small",
    "functional",
    "immutable",
    "lightweight",
    "fast",
    "javascript",
    "library",
    "stdlib",
    "modules"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/concretesolutions/pareto.js"
  },
  "author": "Matheus Lima <matheusml90@gmail.com> (https://matheuslima.com)",
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "testMatch": [
      "**/__tests__/*.js"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "23.0.1",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "jest": "23.1.0",
    "regenerator-runtime": "0.11.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3"
  }
}
